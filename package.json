{
	"name": "cornucopia-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"prebuild": "echo I run before the build script",
		"productionbuild": "vite build && npx svelte-sitemap --out-dir .vercel/output/static --domain https://cornucopia.dotnetlab.eu",
		"build": "vite build && npx svelte-sitemap --domain https://cornucopia.dotnetlab.eu",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"coverage": "vitest --coverage"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.4.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/node": "^20.11.5",
		"@vitest/coverage-v8": "^2.1.1",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-sitemap": "^2.6.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^2.1.1"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@types/js-yaml": "^4.0.9",
		"front-matter": "^4.0.2",
		"js-yaml": "^4.1.0",
		"svelte-markdown": "^0.4.1",
		"sync-request": "^6.1.0",
		"vite-plugin-restart": "^0.4.0",
		"vite-plugin-static-copy": "^1.0.0"
	}
}
